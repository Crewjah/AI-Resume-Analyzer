{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/frontend/assets/$1" },
    { "src": "/test.html", "dest": "/frontend/test.html" },
    { "src": "/(.*)", "dest": "/frontend/index.html" }
  ]
}